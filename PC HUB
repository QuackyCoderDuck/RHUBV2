local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local General = Library.CreateLib("Nightshade", "Synapse")

--General Tab
local Tab = General:NewTab("General")
Section:NewButton("Infinite Yield", "Toggles Infinite Yield", function()
    print("Executing Infinite Yield")
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
    print ("Executed Infinite Yield!")
   end)
