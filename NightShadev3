	Name = "General",
	Icon = "rbxassetid://8569322835"
}

General:Button{
	Name = "Infinite Yield",
	Description = "Executes Infinite Yield",
	Callback = function() 
   print("executing")
   loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
   end
