local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
local Window = Rayfield:CreateWindow({
   Name = "Rayfield Example Window",
   LoadingTitle = "Rayfield Interface Suite",
   LoadingSubtitle = "by Sirius",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "NightShade Hub"
   },
   Discord = {
      Enabled = true,
      Invite = "B6W39g6gw5", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD.
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = false, -- Set this to true to use our key system
   KeySettings = {
      Title = "NightShade Hub",
      Subtitle = "Key System",
      Note = "Join the discord (discord.gg/B6W39g6gw5)",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = "TEST_KEY"
   }
})
local Tab = Window:CreateTab("Test Tab", 4483362458) -- Title, Image
local Button = Tab:CreateButton({
   Name = "Button Example",
   Callback = function()
     print("Test")
   end,
})
